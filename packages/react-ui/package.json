{
  "name": "@resolid/react-ui",
  "version": "0.1.2",
  "private": false,
  "description": "Resolid React components",
  "keywords": [
    "resolid",
    "react",
    "react-hook",
    "tailwindcss"
  ],
  "homepage": "https://ui.resolid.tech",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/resolid/react-ui.git",
    "directory": "packages/react-ui"
  },
  "license": "MIT",
  "author": "Huijie Wei",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./locales/*": {
      "types": "./dist/locales/*.d.ts",
      "import": "./dist/locales/*.js"
    },
    "./tailwindcss": "./tailwindcss/index.css"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "tailwindcss"
  ],
  "scripts": {
    "build": "tsdown",
    "lint": "eslint .",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@floating-ui/react": "^0.27.17",
    "@resolid/utils": "^1.2.0",
    "@tanstack/react-virtual": "^3.13.18",
    "tailwind-variants": "^3.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.28.6",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "babel-plugin-react-compiler": "^1.0.0",
    "jsdom": "^27.4.0",
    "tsdown": "^0.20.1",
    "vitest": "^4.0.18",
    "vitest-axe": "pre"
  },
  "peerDependencies": {
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwindcss": "^4.1.18"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
